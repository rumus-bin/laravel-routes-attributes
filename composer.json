{
  "name": "RumusBin/LaravelAttributesRoute",
  "description": "Laravel attributes route",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "rumus_bin",
      "email": "rumusbin@gmail.com",
      "homepage": "https://github.com/RumusBin"
    }
